@InCollection{Rietz2014,
  author       = "Rietz, J{\"u}rgen and Alves, Cl{\'a}udio and de
                  Carvalho, Jos{\'e} Manuel Val{\'e}rio and Clautiaux,
                  Fran{\c{c}}ois",
  title        = "On the Properties of General Dual-Feasible
