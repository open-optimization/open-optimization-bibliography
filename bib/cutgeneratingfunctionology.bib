@Article{bhk-IPCOext,
  author       = {Amitabh Basu and Robert Hildebrand and Matthias
                  K{\"o}ppe},
  title        = {Equivariant Perturbation in {G}omory and {J}ohnson's
                  Infinite Group Problem---{III}: Foundations for the
                  {$k$}-Dimensional Case with Applications to {$k=2$}},
  journal      = "Mathematical Programming",
  year         = 2017,
  volume       = 163,
  number       = 1,
  pages        = "301--358",
  was-eprint   = {eprint arXiv:{\penalty0}1403.4628 [math.OC]},
  abstract     = "We develop foundational tools for classifying the
                  extreme valid functions for the k-dimensional
                  infinite group problem. In particular, we present
                  the general regular solution to Cauchy's additive
                  functional equation on restricted lower-dimensional
                  convex domains. This provides a k-dimensional
                  generalization of the so-called Interval Lemma,
                  allowing us to deduce affine properties of the
                  function from certain additivity relations. Next, we
                  study the discrete geometry of additivity domains of
                  piecewise linear functions, providing a framework
                  for finite tests of minimality and extremality. We
                  then give a theory of non-extremality certificates
                  in the form of perturbation functions. We apply
                  these tools in the context of minimal valid
                  functions for the two-dimensional infinite group
                  problem that are piecewise linear on a standard
                  triangulation of the plane, under a regularity
                  condition called diagonal constrainedness. We show
                  that the extremality of a minimal valid function is
                  equivalent to the extremality of its restriction to
                  a certain finite two-dimensional group problem. This
                  gives an algorithm for testing the extremality of a
                  given minimal valid function.",
  issn         = "1436-4646",
  doi          = "10.1007/s10107-016-1064-9",
}

