@InProceedings{averkov2014unique,
  author       = "Averkov, Gennadiy and Basu, Amitabh",
  title        = "On the Unique-Lifting Property",
  year         = 2014,
  booktitle    = "Integer Programming and Combinatorial Optimization",
  editor       = "Lee, Jon and Vygen, Jens",
  publisher    = "Springer International Publishing",
  isbn         = "978-3-319-07557-0",
  pages        = "76--87",
  doi          = {10.1007/978-3-319-07557-0_7},
  address      = "Cham",
  abstract     = "We study the uniqueness of minimal liftings of cut
                  generating functions obtained from maximal
                  lattice-free polytopes. We prove a basic invariance
                  property of unique minimal liftings for general
                  maximal lattice-free polytopes. This generalizes a
                  previous result by Basu, Cornu{\'e}jols and
                  K{\"o}ppe [3] for simplicial maximal lattice-free
                  polytopes, thus completely settling this fundamental
                  question about lifting. We also extend results from
                  [3] for minimal liftings in maximal lattice-free
                  simplices to more general polytopes. These
                  nontrivial generalizations require the use of deep
                  theorems from discrete geometry and geometry of
                  numbers, such as the Venkov-Alexandrov-McMullen
                  theorem on translative tilings, and McMullen's
                  characterization of zonotopes.",
}

