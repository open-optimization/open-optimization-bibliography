@Article{bcds-2018:extreme-discontinuous-everywhere,
  author       = "Basu, Amitabh and Conforti, Michele and Di Summa,
                  Marco",
  title        = "An extreme function which is nonnegative and
                  discontinuous everywhere",
  journal      = "Mathematical Programming",
  year         = 2018,
  month        = sep,
  day          = 04,
  abstract     = "We consider Gomory and Johnson's infinite group
                  model with a single row. Valid inequalities for this
                  model are expressed by valid functions and it has
                  been recently shown that any valid function is
                  dominated by some nonnegative valid function, modulo
                  the affine hull of the model. Within the set of
                  nonnegative valid functions, extreme functions are
                  the ones that cannot be expressed as convex
                  combinations of two distinct valid functions. In
                  this paper we construct an extreme function {$pi:
                  \mathbb{R} \to [0,1]$} whose graph is dense in
                  {$\mathbb{R} \times [0,1]$}. Therefore {$\pi$} is
                  discontinuous everywhere.",
  issn         = "1436-4646",
  doi          = "10.1007/s10107-018-1322-0",
  ids          = {bcds_discontinous_everywhere},
  pages        = {1--7},
}

